<%- include("./partials/header") -%>
<div class="container">
  <h1>Home</h1>
  <p><%= homeContent %></p>
  <%posts.forEach(function(post){%>
  <h1><%= post.Title %></h1>
  <p>
    <%= post.Body.substring(0,100)+" ..." %>
    <a href="/posts/<%= post.Title %>">Read More</a>
  </p>
  <% }) %>
</div>
<%- include("./partials/footer") -%>
